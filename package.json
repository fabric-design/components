{
  "name": "styleguide-web-components",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --colors --content-base build webpack.config.js",
    "start:preact": "webpack-dev-server --watch --config webpack.config.preact.js",
    "test": "npm run test:react && npm run test:preact",
    "test:react": "mocha-webpack --webpack-config webpack.config.js \"tests/**/*.spec.js\"",
    "test:preact": "mocha-webpack --webpack-config webpack.config.preact.js \"tests/**/*.spec.js\"",
    "tdd": "mocha-webpack --webpack-config webpack.config.js --watch \"tests/**/*.spec.js\"",
    "lint": "eslint components/**.js components/**.jsx tests/**.js tests/**.jsx",
    "precommit": "npm run lint && npm run test && npm run build",
    "build": "webpack --config webpack.config.publish.js"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.bus.zalan.do/WholesaleDesignSystem/Styleguide-Web-Components.git"
  },
  "keywords": [
    "styleguide",
    "web",
    "components"
  ],
  "files": [
    "components",
    "dist"
  ],
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "format": "esm",
    "directories": {
      "components": "components",
      "dist": "dist"
    },
    "peerDependencies": {
      "preact": "7.1.0",
      "preact-compat": "3.13.1"
    }
  },
  "author": "Fabian Weber",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-react-jsx": "6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "lite-server": "^2.2.0",
    "mocha": "3.2.0",
    "mocha-webpack": "0.7.0",
    "moment": "2.0.0",
    "node-sass": "4.5.0",
    "pre-commit": "1.2.2",
    "preact": "7.1.0",
    "preact-compat": "3.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "5.0.1",
    "sinon": "1.17.7",
    "style-loader": "0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-node-externals": "1.5.4",
    "wholesale-styleguide-scss": "git+ssh://git@github.bus.zalan.do/WholesaleDesignSystem/Styleguide-SCSS.git#dropdown-adjustments",
    "url-loader": "0.5.7"
  },
  "peerDependencies": {
    "preact": "7.1.0",
    "preact-compat": "3.13.1"
  }
}
